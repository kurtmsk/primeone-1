                <input name="file" type="file" />
              </div>
              <div style="float: right;">
                <%= submit_tag "Populate", class:"btn btn-success bg-success" %>
              </div>
            <% end %>
          </div>
        </div>
      </div><!-- col-md-12 -->
    </div><!-- Row -->

