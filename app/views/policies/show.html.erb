  <%= stylesheet_link_tag 'bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css' %>
  <%= stylesheet_link_tag 'dropzone/dropzone.min.css' %>

  <style>
    .buttons {
      border-collapse: collapse;
    }
    .buttons button {
      width: 100%;
    }

    .tab-content {
      padding: 15px 0;
    }

    .tab-pane {
      min-height: 300px;
    }
  </style>
<% end %>

<div class="page-inner">
  <div class="page-breadcrumb">
    <ol class="breadcrumb container">
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Policies", policies_path %></li>
      <li><%= link_to @policy.policy_number, "#", class: 'active policy_number_display' %></li>
    </ol>
  </div>
  <div class="page-title">
    <div class="container">
      <h3><span class="policy_number_display"><%= @policy.policy_number %></span> || <span class="policy_name_display"><%= @policy.name %></span></h3>
    </div>
  </div>
