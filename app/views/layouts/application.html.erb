<!DOCTYPE html>
<html>
<head>
  <!-- Title -->
  <title><%= title(@title) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <meta content="width=device-width, initial-scale=1" name="viewport"/>
  <meta charset="UTF-8">
  <meta name="description" content="Prime One Insurance Dashboard" />
  <meta name="keywords" content="prime,one,primeone,insurance,dashboard" />
  <meta name="author" content="Matthew Vasseur" />

  <!-- Styles -->
  <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Ubuntu:300,400,500,700' %>

  <%= yield :head %>

  <!-- Template use is Modern by Steelcoders -->
  <%#= stylesheet_link_tag 'x-editable/bootstrap3-editable/css/bootstrap-editable.css' %>
  <%#= stylesheet_link_tag 'x-editable/inputs-ext/typeaheadjs/lib/typeahead.js-bootstrap.css' %>
  <%#= stylesheet_link_tag 'x-editable/inputs-ext/address/address.css' %>
  <%#= stylesheet_link_tag 'select2/css/select2.min.css' %>
  <%#= stylesheet_link_tag 'bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css' %>

  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body>

  <%= yield %>


  <%#= javascript_include_tag 'jquery/jquery-2.1.3.min.js' %>
  <%= javascript_include_tag 'jquery-ui/jquery-ui.min.js' %>
  <%= javascript_include_tag 'pace-master/pace.min.js' %>
  <%= javascript_include_tag 'jquery-blockui/jquery.blockui.js' %>
  <%= javascript_include_tag 'bootstrap/js/bootstrap.min.js' %>
  <%= javascript_include_tag 'jquery-slimscroll/jquery.slimscroll.min.js' %>
  <%= javascript_include_tag 'switchery/switchery.min.js' %>
  <%= javascript_include_tag 'uniform/jquery.uniform.min.js' %>
  <%= javascript_include_tag 'classie/classie.js' %>
  <%= javascript_include_tag 'waves/waves.min.js' %>
  <%= javascript_include_tag '3d-bold-navigation/js/main.js' %>
  <%= javascript_include_tag 'modern.min.js' %>
</body>
</html>
